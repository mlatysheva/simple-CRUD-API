    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es6",
    "module": "commonjs",
  },
  "include": ["src"],
  "exclude": ["node_modules", "data"]
}