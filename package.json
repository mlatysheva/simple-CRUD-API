{
  "name": "simple-crud-api",
  "version": "1.0.0",
  "description": "Simple CRUD API based on node js",
  "main": "src/server.ts",
  "scripts": {
    "start:prod": "npm run build && ts-node src/server.ts",
    "start:dev": "nodemon",
    "start:multi": "npm run build && npm run multi",
    "build": "webpack --config webpack.config.js",
    "multi": "cross-env NODE_ENV=production node dist/multi.js",
    "lint": "eslint . --fix",
    "test": "mocha -r ts-node/register 'src/test/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlatysheva/simple-CRUD-API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlatysheva/simple-CRUD-API/issues"
  },
  "homepage": "https://github.com/mlatysheva/simple-CRUD-API#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^10.0.0",
    "stream-http": "^3.2.0",
    "ts-loader": "^9.3.0",
    "url": "^0.11.0",
    "uuid": "^8.3.2"
  }
}
